<!DOCTYPE html>
<html>
<head>
	
  <title>ScholarLyApp</title>
  
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>
  <%= javascript_include_tag 'application' %>
</head>
<body>
	
	<% if current_student %>
	  Signed in as <%= current_student.email %> | <%= link_to "Logout", '/logout' %>
	<% else %>
	  <%= link_to 'Signup', signup_path %>

	  <%= link_to 'Login', '/login' %>
	<% end %>

<%= yield %>


	</body>
</html>
